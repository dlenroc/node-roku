{
  "compilerOptions": {
    "lib": ["ES2019", "ES2020.Promise", "ES2020.BigInt", "ES2020.String"],
    "target": "ES2019",
    "sourceMap": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "declarationMap": true,
    "declarationDir": "./",
    "paths": {
      "@dlenroc/*": ["./node_modules/@dlenroc/*"]
    }
  },
  "exclude": ["./dist", "./node_modules"]
}
