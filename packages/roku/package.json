{
  "name": "@dlenroc/roku",
  "version": "1.2.0",
  "description": "All roku control protocols via one client",
  "author": "Corneliu Duplachi",
  "license": "MIT",
  "repository": "dlenroc/node-roku",
  "keywords": [
    "roku",
    "sdk"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
